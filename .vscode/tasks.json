{
    "version": "0.1.0",
    "command": "npm",
    "isShellCommand": true,
    "showOutput": "always",
    "args": [
        "run"
    ],
    "echoCommand": true,
    "tasks": [
        {
            "taskName": "start",
            "isBuildCommand": true,
            "showOutput": "always",
            "isBackground": true,
            "isTestCommand": false,
            "problemMatcher": {
                "owner": "angular-cli",
                "fileLocation": "relative",
                "pattern": {
                    "regexp": "^([^\\s].*)\\((\\d+|\\,\\d+|\\d+,\\d+,\\d+,\\d+)\\):\\s+(error|warning|info)\\s+(TS\\d+)\\s*:\\s*(.*)$",
                    "file": 1,
                    "location": 2,
                    "severity": 3,
                    "code": 4,
                    "message": 5
                },
                "background": {
                    "activeOnStart": true,
                    "beginsPattern": "NG Live Development Server",
                    "endsPattern": "^Time:"
                }
            }
        },
        {
            "taskName": "test",
            "isBuildCommand": false,
            "showOutput": "always",
            "isBackground": true,
            "isTestCommand": true
        }
    ]
}
